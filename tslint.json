{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [true],
        "max-line-length": [true, {"limit": 140, "ignore-pattern": "^import | //"}],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "ordered-imports": false
    },
    "rulesDirectory": []    
}